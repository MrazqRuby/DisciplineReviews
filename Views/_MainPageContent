<script type="text/javascript">
    $("#make-criteria").on("click", function(){
        $.ajax({
            type: "POST",
            url: "/Course/_MakeCriteriaForm",
            context:this,
            success: function(msg){
                $("#main_content").html(msg);
            }
        });
    });
</script>